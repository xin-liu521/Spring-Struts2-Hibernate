<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.velvol.hr.domain.Leavereq" table="leavereq" >
        <id name="id">
            <generator class="native"/>
		</id>
       <property name="preworkerid" type="java.lang.String">
            <column name="preworkerid" length="60" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="20" />
        </property>
        <property name="reason" type="java.lang.String">
            <column name="reason" length="200" />
        </property>
        
         <property name="reqdate" type="java.util.Date">
            <column name="reqdate" length="0" />
        </property> 
        <property name="leavedate" type="java.util.Date">
            <column name="leavedate" length="0" />
        </property> 
        
         <property name="regionid" />
        <property name="stationid" />
         <property name="state" />
        <property name="reqtype" />
        <property name="who" />
        
               
    </class>
    
</hibernate-mapping>
