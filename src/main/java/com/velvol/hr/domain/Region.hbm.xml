<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.velvol.hr.domain.Region" table="region" >
        <id name="id">
            <generator class="native"/>
		</id>
			
        <property name="name" type="java.lang.String">
            <column name="name" length="20" />
        </property>
        <property name="addr" type="java.lang.String">
            <column name="addr" length="100" />
        </property>
        <property name="regionbh" type="java.lang.String">
            <column name="regionbh" length="20" />
        </property>
        
         <property name="manager" type="java.lang.String">
            <column name="manager" length="20" />
        </property>
        <property name="managertel" type="java.lang.String">
            <column name="managertel" length="20" />
        </property>
        
        <property name="linkman" type="java.lang.String">
            <column name="linkman" length="20" />
        </property>
        <property name="linkmantel" type="java.lang.String">
            <column name="linkmantel" length="20" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="200" />
        </property>
       
       
    </class>
    
</hibernate-mapping>
