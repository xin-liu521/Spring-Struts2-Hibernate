<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.velvol.hr.domain.Kaoqinwh" table="kaoqinwh" >
        <id name="id">
            <generator class="native"/>
		</id>

		<property name="stationid" />
		 <property name="stationname" type="java.lang.String">
            <column name="stationname" length="20" />
        </property>
        <property name="workerid" type="java.lang.String">
            <column name="workerid" length="32" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="20" />
        </property>
        <property name="meituanid" type="java.lang.String">
            <column name="meituanid" length="20" />
        </property>
        <property name="telephone" type="java.lang.String">
            <column name="telephone" length="20" />
        </property>
        
		<property name="kqdate" type="java.util.Date">
            <column name="kqdate" length="0" />
        </property> 
        <property name="state" />   
        <property name="issubmit" />    
        <property name="remark" type="java.lang.String">
            <column name="remark" length="200" />
        </property>    
        
         <property name="zcstate" />  
        <property name="btstate" />  
        <property name="yxstate" />     
    </class>
    
</hibernate-mapping>
