<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.velvol.hr.domain.Tipinfo" table="tipinfo" >
        <id name="id">
            <generator class="native"/>
		</id>
		
		<property name="type" />
		
        <property name="content" type="java.lang.String">
            <column name="content" length="200" />
        </property>
       <property name="regionid" />
       
       <property name="name" type="java.lang.String">
            <column name="name" length="20" />
        </property>
        <property name="linkman" type="java.lang.String">
            <column name="linkman" length="20" />
        </property>
        <property name="linkmantel" type="java.lang.String">
            <column name="linkmantel" length="20" />
        </property>
        <property name="addr" type="java.lang.String">
            <column name="addr" length="100" />
        </property>
       
    </class>
    
</hibernate-mapping>
